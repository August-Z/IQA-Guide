{
  "title": "IQA-Engine",
  "author": "August-Z",
  "root": ".",
  "plugins": [
    "-sharing",
    "sharing-plus",
    "prism",
    "github",
    "github-buttons"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/August-Z/IQA-Guide"
    },
    "github-buttons": {
      "buttons": [{
        "user": "kuangshp",
        "repo": "mysql",
        "type": "star",
        "size": "small",
        "count": true
      }]
    },
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": false,
      "hatenaBookmark": false,
      "instapaper": false,
      "line": false,
      "linkedin": false,
      "messenger": false,
      "pocket": false,
      "qq": true,
      "qzone": true,
      "stumbleupon": false,
      "twitter": false,
      "viber": false,
      "vk": false,
      "weibo": true,
      "whatsapp": false,
      "all": [
        "facebook",
        "google",
        "twitter",
        "weibo",
        "qq",
        "linkedin",
        "qzone",
        "douban"
      ]
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "fontsettings": {
      "theme": "white"
    }
  }
}
